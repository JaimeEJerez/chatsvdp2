<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <stroke android:width="1dip" android:color="#FC7400" />
            <gradient android:angle="-90" android:startColor="#FC7400" android:endColor="#FC7400"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <stroke android:width="1dip" android:color="#FC7400" />
            <solid android:color="#FC7400"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <stroke android:width="1dip" android:color="#FC7400" />
            <gradient android:angle="-90" android:startColor="#FC7400" android:endColor="#FC7400" />
        </shape>
    </item>
</selector>